    "chai": "^4.3.0",
    "chai-as-promised": "^7.1.1",
    "eslint": "7.2.0",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-plugin-import": "2.22.1",
    "fetch-mock": "9.11.0",
    "get-parameter-names": "^0.3.0",
    "gulp": "^4.0.2",
